	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"homepage": "https://github.com/baufometic/maximilian#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/baufometic/maximilian.git"
	},
	"bugs": {
		"url": "https://github.com/baufometic/maximilian/issues"
	},
	"main": "build/index.js",
	"module": "build/index.esm.js",
	"types": "build/index.d.ts",
	"files": [
		"build"
	],
	"keywords": [
		"maximilian",
		"react",
		"js",
		"javascript"
	],
	"scripts": {
		"removebuild-n-bump": "rm -rf build && npm version minor",
		"removeall-n-bump": "rm -rf build && rm -rf node_modules && npm version minor",
		"build-dev": "npm run removebuild-n-bump && NODE_ENV=development rollup -c",
		"build-dev:watch": "npm run removebuild-n-bump && NODE_ENV=development rollup -c -w",
		"build-prod": "npm run removebuild-n-bump && NODE_ENV=production rollup -c",
		"reinstall": "removeall-n-bump && npm i",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"styled-components": "^5.3.5"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^21.0.3",
