{
	"name": "@techandtribal/maximilian",
	"description": "Personal toolkit for JS",
	"version": "1.60.0",
	"author": "Pete Savva <p.savva@protonmail.ch> (https://techandtribal.com)",
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"homepage": "https://github.com/baufometic/maximilian#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/baufometic/maximilian.git"
	},
	"bugs": {
		"url": "https://github.com/baufometic/maximilian/issues"
	},
	"source": "src/index.ts",
	"main": "build/index.js",
	"types": "build/index.d.ts",
	"files": [
		"build"
	],
	"keywords": [
		"maximilian",
		"react",
		"js",
		"javascript"
	],
	"scripts": {
		"bump": "npm version minor --no-git-tag-version",
		"build-dev": "npm run bump && NODE_ENV=development parcel build",
		"build-new": "npm run bump && npm run remove-parcel-files && npm run build-dev",
		"build-prod": "npm run bump && NODE_ENV=production parcel build",
		"reinstall": "npm run remove-npm-files && npm run remove-parcel-files && npm run bump && npm i",
		"remove-npm-files": "rm -rf node_modules",
		"remove-parcel-files": "rm -rf .parcel-cache build",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"peerDependencies": {
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"styled-components": "^5.3.5"
	},
	"devDependencies": {
		"@parcel/packager-ts": "^2.5.0",
		"@parcel/transformer-typescript-types": "^2.5.0",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.1.1",
		"@testing-library/user-event": "^14.1.1",
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.29",
		"@types/react": "^18.0.8",
		"@types/react-dom": "^18.0.0",
		"@types/styled-components": "^5.1.25",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.5.0",
		"identity-obj-proxy": "^3.0.0",
		"parcel": "^2.5.0",
		"ts-jest": "^27.1.4",
		"typescript": "^4.6.3"
	}
}
